                "#1\r\n",
                "import numpy as np\r\n",
                "import matplotlib.pyplot as plt\r\n",
                "\r\n",
                "X = np.linspace(-np.pi, np.pi, 256, endpoint = True)\r\n",
                "C, S = np.cos(X), np.sin(X)\r\n",
                "\r\n",
                "fig, ax = plt.subplots()\r\n",
                "ax.plot(X, C)\r\n",
                "ax.plot(X, S)\r\n",
                "plt.show()"
            ],
